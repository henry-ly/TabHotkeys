{
  "name": "Tab Navigation",
  "action": {},
  "manifest_version": 3,
  "version": "0.1",
  "description": "Hotkeys for managing tabs",
  "permissions": ["tabs","activeTab", "browsingData"],
  "background": {
    "service_worker": "service-worker.js"
  },
  "commands": {
    "clear_tabs": {
      "suggested_key": "Ctrl+M",
      "description": "Clear all tabs"
    },
    "remove_previous": {
      "suggested_key": "Ctrl+Shift+L",
      "description": "Remove second recent tabs"
    },
    "remove_first": {
      "suggested_key": "Ctrl+Shift+X",
      "description": "Remove first tab"
    },
    "remove_last": {
      "suggested_key": "Ctrl+Shift+F",
      "description": "Remove last tab"
    }
  }

}
